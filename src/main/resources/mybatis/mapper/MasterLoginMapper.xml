<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.aynu.SpringBoot.mapper.MasterLoginMapper">

    <select id="findMasterByNameAndPassword" resultType="Master">
        select * from master where name=#{name} and password=#{password}
    </select>
</mapper>